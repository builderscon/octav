[% WRAPPER "layout/main.html.tx" %]

<div class="row">
  <div class="input-field s12">
    <ul class="collection with-header">
      <li class="collection-header"><h4>Rooms</h4></li>
[% FOREACH room IN rooms %]
      <li class="collection-item"><a href="/room/lookup?id=[% room.id %]">[% room.name %]</a></li>
[% END %]
    </ul>
  </div>
</div>

[% END %]